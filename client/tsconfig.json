{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "esnext",
		"target": "es5",
		"jsx": "react",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		// Overwrite default lib to add esnext, solves async errors
		"lib": ["es6", "dom", "esnext"]
	}
}
