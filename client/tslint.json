{
	"defaultSeverity": "warning",
	"extends": ["tslint-config-airbnb", "tslint-react-recommended"],
	"jsRules": {},
	"rules": {
		"variable-name": false,
		"indent": [true, "tabs", 2],
		"import-name": false,
		"ter-indent": false,
		"no-boolean-literal-compare": true,
		"quotemark": [true, "double"],
		"align": [true, "parameters", "**statements**"]
	},
	"rulesDirectory": [],
	"linterOptions": {
		"exclude": ["node_modules"]
	}
}
