{
	"name": "portfolio-site-server",
	"version": "1.0.0",
	"description": "",
	"main": "start.js",
	"scripts": {
		"start": "env-cmd ./config/dev.env nodemon ./src/index.js --ext js,graphql --exec babel-node",
		"get-schema": "graphql get-schema -p prisma",
		"prisma-deploy-prod": "cd prisma && prisma deploy -e ../config/prod.env",
		"prisma-deploy-dev": "cd prisma && prisma deploy -e ../config/dev.env",
		"prisma-deploy-test": "cd prisma && prisma deploy -e ../config/test.env",
		"test": "env-cmd ./config/test.env jest --watch",
		"test-ci": "jest"
	},
	"jest": {
		"globalSetup": "./tests/jest/globalSetup.js",
		"globalTeardown": "./tests/jest/globalTeardown.js"
	},
	"babel": {
		"presets": [
			"env"
		],
		"plugins": [
			"transform-object-rest-spread"
		]
	},
	"author": "Floris Warmenhoven",
	"license": "ISC",
	"dependencies": {
		"@babel/polyfill": "^7.0.0",
		"apollo-boost": "^0.1.22",
		"babel-cli": "^6.26.0",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.3",
		"cookie-parser": "^1.4.3",
		"cors": "^2.8.5",
		"cross-fetch": "^3.0.0",
		"dotenv": "^6.2.0",
		"env-cmd": "^8.0.2",
		"express": "^4.16.4",
		"graphql": "^14.0.2",
		"graphql-cli": "^3.0.1",
		"graphql-yoga": "^1.16.7",
		"jest": "^23.6.0",
		"jsonwebtoken": "^8.4.0",
		"prisma-binding": "^2.2.10"
	},
	"devDependencies": {
		"nodemon": "^1.18.9"
	}
}
