{
	"name": "portfolio-site-root",
	"version": "1.0.0",
	"description": "[![CircleCI](https://circleci.com/gh/FlorisWarmenhoven/portfolio-site/tree/master.svg?style=shield)](https://circleci.com/gh/FlorisWarmenhoven/portfolio-site/tree/master)",
	"main": "index.js",
	"engines": {
		"node": "11.x"
	},
	"scripts": {
		"start": "env-cmd server/config/prod.env babel-node server/src/index.js",
		"dev": "concurrently \"cd client && npm start\" \"cd server && npm start\"",
		"heroku-postbuild": "cd client && npm install && npm install -only=dev && npm run build && cd ../server && npm install && npm run get-schema"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-proposal-object-rest-spread"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FlorisWarmenhoven/portfolio-site.git"
	},
	"author": "Floris Warmenhoven",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/FlorisWarmenhoven/portfolio-site/issues"
	},
	"homepage": "https://github.com/FlorisWarmenhoven/portfolio-site#readme",
	"dependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"env-cmd": "^8.0.2"
	},
	"devDependencies": {
		"concurrently": "^4.1.0"
	}
}
